<div
  <% if let Some(id) = id { %> id="<%= id %>" <% } %>
  data-file-type="web_builder"
  class="flex flex-col justify-center items-center gap-1.5 hover:bg-blue-900 group-hover/item:bg-blue-900 p-1.5 rounded-xs w-24 h-fit file"
>
  <img
    class="w-9 h-9"
    src="/assets/images/web-builder/web-builder.svg"
    draggable="false"
  />
  <div
    class="max-w-20 overflow-ellipsis text-white text-sm text-center line-clamp-2"
  >
    Web Builder
  </div>
</div>

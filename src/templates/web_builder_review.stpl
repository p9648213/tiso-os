<% if review_mode == ReviewMode::Preview { %>
  <link rel="stylesheet" href="/assets/css/lib/tailwind.css" />
<% } %>

<% if review_mode == ReviewMode::Download { %>
  <link rel="stylesheet" href="styles.css" />
<% } %>

<div id="builder-review" class="flex-1 overflow-auto">
  <div class="min-w-xl">
      <% for child in &body_node.children { %>
        <% let node = nodes.get(child).unwrap(); %>
        <% include!("web_builder_node.stpl"); %>
      <% } %>
  </div>
</div>
